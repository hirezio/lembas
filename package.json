{
  "name": "@hirez_io/lembas",
  "version": "0.0.0-development",
  "repository": {
    "type": "git",
    "url": "https://github.com/hirezio/lembas"
  },
  "description": "A simple little class that helps making Observable testing a breeze",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "commit": "git-cz",
    "generate-toc": "doctoc README.md",
    "format:fix": "pretty-quick --staged",
    "lint": "eslint . --ext .js,.ts",
    "semantic-release": "semantic-release",
    "prepare": "husky install",
    "db": "nx db demo-server",
    "server": "nx serve demo-server",
    "server-and-db": "run-p db server",
    "demo": "nx run-many --target=serve --projects=demo,demo-server",
    "test:smoke:watch": "run-p server-and-db cy:open",
    "test:smoke": "run-p server-and-db cy:run",
    "cy:open": "nx e2e lembas-e2e --watch",
    "cy:run": "nx e2e lembas-e2e",
    "postinstall": "ngcc --properties es2015 browser module main"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "db seeding",
    "smoke testing",
    "smoke tests",
    "e2e tests",
    "database seeding",
    "data seeding"
  ],
  "author": {
    "name": "Shai Reznik",
    "company": "HiRez.io"
  },
  "license": "MIT",
  "workspaces":[
    "packages/lembas",
    "packages/lembas-cypress"
  ],
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular-eslint/eslint-plugin": "~13.1.0",
    "@angular-eslint/eslint-plugin-template": "~13.1.0",
    "@angular-eslint/template-parser": "~13.1.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@hirez_io/jest-single": "1.1.5",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/angular": "14.1.7",
    "@nrwl/cli": "14.1.5",
    "@nrwl/cypress": "14.1.5",
    "@nrwl/jest": "14.1.5",
    "@nrwl/js": "14.1.5",
    "@nrwl/nest": "14.1.7",
    "@nrwl/node": "14.1.7",
    "@nrwl/workspace": "14.1.5",
    "@types/jest": "^27.4.1",
    "@types/mock-fs": "4.13.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "all-contributors-cli": "6.20.0",
    "axios": "0.27.2",
    "commitizen": "4.2.4",
    "cypress": "9.6.1",
    "cz-conventional-changelog": "3.3.0",
    "doctoc": "2.1.0",
    "eslint": "8.10.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-cypress": "^2.10.3",
    "execa": "5.1.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-preset-angular": "11.1.1",
    "memfs": "3.4.4",
    "mock-fs": "5.1.2",
    "npm-run-all": "^4.1.5",
    "nx": "14.1.5",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "prisma": "3.14.0",
    "rimraf": "3.0.2",
    "rxjs": "~7.5.4",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "9.1.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@prisma/client": "3.14.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  }
}
